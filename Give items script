local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local w = library:CreateWindow("Give Items") -- Creates the window

local b = w:CreateFolder("Guns") -- Creates the folder(U will put here your buttons,etc)

b:Label("Made by dfsaf#1575/lmmake",{
    TextSize = 15; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 

b:Button("Give AK47",function()
local GunRep = game.ReplicatedStorage.Tools.AK47:Clone()
GunRep.Parent = workspace[game.Players.LocalPlayer.Name]
end)
b:Button("Give Famas",function()
local GunRep1 = game.ReplicatedStorage.Tools.FAMAS:Clone()
GunRep1.Parent = workspace[game.Players.LocalPlayer.Name]
end)
